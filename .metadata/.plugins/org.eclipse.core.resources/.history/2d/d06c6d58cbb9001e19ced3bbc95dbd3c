package my.day06.a.multifor;

public class 김승혜_Main_homework_5 {

	public static void main(String[] args) {
		
		System.out.println("\n == 숙제 1 ==\n");
		
	
		for(int i=1; i<=5; i++) { 
			for(int d = 1 ; d <= i; d++) 
				System.out.print("*");
				System.out.print("\n");
			}// end of for----
			
		
		System.out.println("\n == 숙제 2 ==\n");
		for(int i=1; i<=5; i++) { 
			for(int d = 1 ; d <= i; d++) 
				System.out.printf("*");
				System.out.print("\n");
			}
	
	
	
	
	
	}	
	

}

	


