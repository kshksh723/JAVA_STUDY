package my.day06.a.multifor;

public class Main_multifor_1 {

	public static void main(String[] args) {
		// 단일 for문으로 아래와 같이 나오도록 한다
		/*
		 abcdfg
		 hijklmn
		 opqrstu
		 */
		
		char ch = 'a';
		for(int i=0; i<'u'-'a'+1; i++) {
			System.out.print(ch++); //ch=(char)(ch+1);
		} // end of for -------------
		
	}// end of main()------------------

}
